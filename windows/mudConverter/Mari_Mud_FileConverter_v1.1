    def getFileInfo(self, file):
                        # Expects absolute path and returns it's path, filename, format and name without the format
                        #print(file)
                        pathSplit = file.split('/')[:-1]
                        path = '/'.join(pathSplit)
                        #print(path)
                        fileName = file.split('/')[-1]

                        fileFormat = fileName.split('.')[-1]
                        withoutFormat = fileName.split('.')[:-1]
                        withoutFormat = withoutFormat[-1] # removes list and gets the file name without the format
                        #print(path,fileName,fileFormat,withoutFormat)
                        return path, fileName, fileFormat, withoutFormat

    def mari2Mud(self, fileSelection):
        replace =self.replace_btn.isChecked()
