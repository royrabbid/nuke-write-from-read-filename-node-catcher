            #print('withoutFormat',withoutFormat)
            try:
                udimValue = int(withoutFormat.split('_')[-1])
                nameWithoutUdim = (withoutFormat.split('_')[:-1])
                if len(nameWithoutUdim) > 1:
                    nameWithoutUdim = '_'.join(nameWithoutUdim)
                else:
                    nameWithoutUdim = nameWithoutUdim[0]
                uValue,vValue = self.getUV(udimValue)[0],self.getUV(udimValue)[-1]
                newName = path + '/' + nameWithoutUdim + '_' + 'u%d_v%i' %(uValue,vValue) + '.' + str(fileFormat)
